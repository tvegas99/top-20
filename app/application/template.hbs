{{#paper-nav-container}}
  {{#paper-sidenav class="md-sidenav-left md-whiteframe-z2" locked-open="gt-sm"}}
    {{#paper-toolbar}}
        <div class="md-toolbar-tools">
          <div class="logo">
            <strong>EBE's Top 20</strong>
          </div>
        </div>
    {{/paper-toolbar}}
    {{#paper-content}}
      {{#paper-list}}
        {{#paper-item action="transitionTo" param="basic-info"}}View your Basic Info{{/paper-item}}
        {{#paper-item action="transitionTo" param="timeline"}}View your Timeline{{/paper-item}}
        {{#paper-item action="transitionTo" param="song-selections"}}View your Song Selections{{/paper-item}}
      {{/paper-list}}
    {{/paper-content}}
  {{/paper-sidenav}}

  <div layout="column" tabindex="-1" role="main" flex>
    {{#paper-toolbar}}
      <div class="md-toolbar-tools">
        {{#paper-sidenav-toggle as |toggle|}}
          {{#paper-button target=toggle action="toggleMenu" icon-button=true}}
            {{paper-icon icon="menu"}}
          {{/paper-button}}
        {{/paper-sidenav-toggle}}
        <span class="md-breadcrumb-page">Title TODO</span>
      </div>
    {{/paper-toolbar}}
    {{#paper-content class="md-padding"}}
      <h2 id="title">Welcome to EBE's Top 20</h2>

      <p>your place to communicate with your EBE bandleader</p>
      
      {{outlet}}
    {{/paper-content}}
  </div>
{{/paper-nav-container}}
